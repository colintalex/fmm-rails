<%= form_with model: @user, url: '/register', method: "post" do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :password_digest %>
  <%= f.text_field :password_digest %>
  <%= f.label :password_confirmation %>
  <%= f.text_field :password_confirmation %>
  <%= f.submit %>
<% end %>


<h1>
<%= User.all.length %>
</h1>